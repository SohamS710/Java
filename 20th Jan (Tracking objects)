class Counter {
    static int count = 0;
    Counter() {
        count++;
        System.out.println("Object Created. Counter: " + count);
    }
}

public class Main {
    public static void main(String[] args) {
        Counter obj1 = new Counter();
        Counter obj2 = new Counter();
        Counter obj3 = new Counter();
        Counter obj21 = new Counter();
        Counter obj31 = new Counter();

        System.out.println("Final Count: " + Counter.count);
    }
}
