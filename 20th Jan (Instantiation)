class Student {
    String name;
    int gpa;

    Student(String n, int a) {
        name = n;
        gpa = a;
    }

    void display() {
        System.out.println("Name: " + name + ", GPA: " + gpa);
    }
}

public class Main {
    public static void main(String[] args) {
        Student student1 = new Student("Alice", 3);
        student1.display();
    }
}
